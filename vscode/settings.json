    "editor.lineHighlightBackground": "#F000FF40",
    "editor.selectionBackground": "#F000FF80",
    "editor.selectionHighlightBackground": "#FF0000A0",
    "editorInfo.foreground": "#88ff00",
    "editorOverviewRuler.infoForeground": "#00000000",
    "editorWarning.foreground": "#FFAA00"
  },
  "workbench.colorTheme": "Monokai",
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.tabCloseButton": "off",
  "workbench.iconTheme": "vscode-icons",
  "zenMode.centerLayout": false
